{
  "version": 2,
  "name": "pricing-agent",
  "builds": [
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    },
    {
      "src": "app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { 
      "src": "/(.*)", 
      "dest": "/app.js" 
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}