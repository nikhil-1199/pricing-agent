{
  "version": 2,
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.js"
    }
  ],
  "env": {
    "FIRECRAWL_API_KEY": "@firecrawl_api_key",
    "PERPLEXITY_API_KEY": "@perplexity_api_key",
    "SERPER_API_KEY": "@serper_api_key"
  }
}